                                "list_": [],
                                "text_": "Upload logo,Teacher photo\n"
                            },
                            "body": {
                                "_id_": 244,
                                "_type": "Sequence",
                                "list_": [
                                    {
                                        "_id_": 243,
                                        "_type": "Activity.Start",
                                        "showChooser": {
