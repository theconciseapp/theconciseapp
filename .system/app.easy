                                                    "value_": "android.webView.setAllowExternalURLs(false);\n \nvar pconfig={\n  \"vendor\":\"rave\" \/\/\"rave\" \"paystack\"\n}\n  \n\nfunction m_bl(){ return 0; }\n\n\nfunction showReload(){\n   $('.pay').html('<button class=\"retry btn btn-sm btn-warning\" onclick=\"pwr();\">Retry<\/button>');\n  }\n       \n function userForm(x){\n   $('.views').hide();\n   var prevCustFname=$.trim(localStorage.getItem('prouser_fullname'));\n   var prevCustPhone=$.trim(localStorage.getItem('prouser_phone'));\n      var prevCustEmail=$.trim(localStorage.getItem('prouser_email'));\n\n     if(prevCustFname) $('#fname').val(prevCustFname);\n    if(prevCustPhone) $('#phone').val(prevCustPhone);\n   if(prevCustEmail) $('#email').val(prevCustEmail);\n   \n   if( !prevCustEmail ||x){\n     $(\".user-form\").fadeIn();\n     $(\".user-payment-info\").css(\"display\",\"none\");\n   }\n  \n  if(x) $(\"#close_gamer_form\").show();\n     else {\n      $(\"#close_gamer_form\").hide(); \n     }\n   }\n   \nfunction saveUserForm(){    \n    var fn=$.trim( $('#fname').val()  );\n    var pn=$.trim($('#phone').val()  );   \n    var e=$.trim( $('#email').val() );\n   var name=fn.split(' ');\n    if(name.length<2){\n      toast('Please enter your full name.');\n      return false;\n    }\n    \n  if(!fn.match(\/^[a-zA-Z ]{2,100}$\/)){\n    toast('Please enter a valid fullname.');\n   return false;\n  }   \n  else     \n  if(!pn.match(\/^[0-9+-]{5,20}$\/)){\n     toast('Please enter a valid phone number.');\n   return false;\n  }       \n   \n else if(e.length<4) {\n     toast('Enter a valid email address.');\n   return false;\n }\n    \n   localStorage.setItem('prouser_fname',name[0]);\n   localStorage.setItem('prouser_fullname',fn);\n   localStorage.setItem('prouser_lname',name[1]);\n   localStorage.setItem('prouser_phone',pn);\n   localStorage.setItem('prouser_email',e);\n  \n  $(\".save\").text(\"Saving...\");\n   setTimeout(function(){\n     location.reload(); \n   },1500);\n  }\n   \nfunction helpline(v){\n if(v) {\n   openInBrowser(whatsApp('*GId: ' +appId() + ',#BET' + randomString(3) + APP_VERSION + '*\\nHello! Request for activation code.\\n\\n_Kindly drop your previous payment evidence or your name & email address and month of payment_') );\n  return false;\n  }\n  openInBrowser(whatsApp('*GId: ' + appId()  + ',#BET' + randomString(3) + APP_VERSION + '*\\nHello! There is a problem activating the app\\n\\n_Please state the error or issue below_'));\n}\n \n      \n function acceptCondition(){\n    localStorage.setItem('accept_upgrade_condition','yes');\n   userForm();\n   }\n    \n \n function tac(){\n   userForm();\n   return;\n if(localStorage.getItem('accept_upgrade_condition') ){\n   userForm();\n     return false;\n   }\n   $('.views').hide();\n   $('.tac').fadeIn();\n }\n\n \n function pwr(){\n      \n   var custFirstname=$.trim(localStorage.getItem('prouser_fname') );\n   var custLastname=$.trim(localStorage.getItem('prouser_lname'));\n   var custPhone=$.trim(localStorage.getItem('prouser_phone') );\n   var custEmail=$.trim( localStorage.getItem('prouser_email'));\n   var pid=localStorage.getItem(\"selected_book_id\")||\"\";\n\n  var amt=+localStorage.getItem(pid + \"_payment_amount\")||1000;\n  var cexp=+localStorage.getItem(\"coins_expected\")||1000;\n \n  var currentBal=+m_bl(); \n   var buypro=localStorage.getItem(\"buy_pro\");\n   \n if(!custFirstname||!custLastname||!custPhone||!custEmail||custEmail=='null') {\n   userForm();\n   return false;\n    }\n \nvar drate= localStorage.getItem(\"dollar-rate\")||460;\n   \n  var udata='<div style=\"font-size: 70%;\">';\n    \n \/\/  udata+='<div class=\"text-center\"><button class=\"change_cust_email\" style=\"background: #004480;\">change<\/button><\/div>';\n   \n\n     udata+='<div>Name: ' + custFirstname + ' ' + custLastname + '<\/div>'; \n     udata+='<div>Email: ' + custEmail + '<\/div>'; \n     udata+='<div>Phone: ' + custPhone + '<\/div>'; \n  