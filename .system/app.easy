                                        },
                                        "intent": {
                                            "_id_": 214,
                                            "_type": "Intent",
                                            "action": {
                                                "_id_": 212,
                                                "_type": "String",
                                                "value_": "ACTION_GET_CONTENT"
                                            },
                                            "type": {
                                                "_id_": 213,
                                                "_type": "String",
                                                "value_": "image\/*"
                                            }
                                        },
                                        "onResult": {
                                            "_id_": 242,
                                            "_type": "Sequence",
                                            "list_": [
                                                {
                                                    "_id_": 219,
                                                    "_type": "InlineScript",
                                                    "code": {
                                                        "_id_": 218,
                                                        "_type": "String",
                                                        "value_": "var importedFiles=[];\n\nvar uri = intent.getData();\n\n if(uri) goImportPostFiles_(uri,0);\n\nvar clip = intent.getClipData();\n   if(clip) {\n  var n = clip.getItemCount();\n  if(n>6) n=6;\n  \n  for(var i=0; i<n; ++i) {\n    uri = clip.getItemAt(i).getUri();\n    if(uri) {\n     goImportPostFiles_(uri,i);\n    }\n  }  \n}  \n\nfunction file_ext(file) {\n    var extension = file.substr( (file.lastIndexOf('.') +1) );\n    switch(extension) {\n        case 'jpg':\n        case 'png':\n        case 'gif':\n        case 'jpeg':\n            return 'jpg';\n        break;\n        default:\n        return extension;\n    }\n}\n\n\nfunction goImportPostFiles_( uri,pos) {  \n  try{\n var name = com.extra.Extra.getNameForUri(context,uri);\n var type=  file_ext( name);\n    if(!type ) return;\n    type=type.toLowerCase();\n    \n    \/\/name=randomNumber( 15) + '.' + type;\n    \n   name= pos + '.' + type;\n         \n  var folder = new java.io.File( importFolder,'TEMP-DIR');\n  var file  =  new java.io.File(folder, name);\n  var thumb  =  new java.io.File(folder, \"thumb.png\");\n\n   folder.mkdirs();\n\n   var is = context.getContentResolver().openInputStream(uri);\n   \n  if(type!='jpg' ){\n    var os = new java.io.FileOutputStream(file);\n   com.serakont.ab.IOUtils.copyStream(is, os, true, true);\n  } else{\n   decodedImage=resizeImage(is);\n   compressImage( decodedImage, file, PHOTO_FORMAT);\n   imageThumbnail( decodedImage, thumb);\n }\n  importedFiles.push( file.toString() );        \n  importedFiles.push( thumb.toString() );        \n \n  }\n  catch(err) {\n    console.log(err);\n    if(file && file.isFile()){\n      file.delete();\n    }\n  }\n}\n"
                                                    }
                                                },
